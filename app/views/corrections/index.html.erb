<% noindex %>
<% nofollow %>

<h1>All Corrections</h1>
<h2> Drafts </h2>
<% if @corrections_draft.any? %>
	<ul class="users alert-heading" %>
		<% @corrections_draft.each do |c| %>
			<li>
				<%= c.editor.full_name_us %> on <%= c.updated_at %> editted
				Post #<%= link_to c.corrected_post.id, correction_path(c) %>
			</li>
		<% end %>
	</ul>
<% end %>
<h2> Published </h2>
<% if @corrections_published.any? %>
	<ul class="users alert-heading" %>
		<% @corrections_published.each do |c| %>
			<li>
				<%= c.editor.full_name_us %> on <%= c.updated_at %> editted
				Post #<%= link_to c.corrected_post.id, correction_path(c) %>
			</li>
		<% end %>
	</ul>
<% end %>