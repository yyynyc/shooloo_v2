<% provide(:title, 'All Members') %>
<h1>All Shooloo Members</h1>

<%= search_form_for @search, url: search_users_path, 
		method: :post do |f| %>
	<%= render partial: 'users/user_search', locals: {f: f} %>
<% end %>

<% @users.each do |user| %>
	<%= render partial: 'users/user_list', locals: {user: user} %>
<% end %>
<%= will_paginate @users %>
