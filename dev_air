--
-- PostgreSQL database cluster dump
--

SET client_encoding = 'UTF8';
SET standard_conforming_strings = on;

--
-- Roles
--

CREATE ROLE shooloo;
ALTER ROLE shooloo WITH NOSUPERUSER INHERIT NOCREATEROLE NOCREATEDB LOGIN NOREPLICATION;
CREATE ROLE yyynyc;
ALTER ROLE yyynyc WITH SUPERUSER INHERIT CREATEROLE CREATEDB LOGIN REPLICATION;






--
-- Database creation
--

CREATE DATABASE shooloo_development WITH TEMPLATE = template0 OWNER = shooloo;
CREATE DATABASE shooloo_test WITH TEMPLATE = template0 OWNER = shooloo;
REVOKE ALL ON DATABASE template1 FROM PUBLIC;
REVOKE ALL ON DATABASE template1 FROM yyynyc;
GRANT ALL ON DATABASE template1 TO yyynyc;
GRANT CONNECT ON DATABASE template1 TO PUBLIC;


\connect postgres

